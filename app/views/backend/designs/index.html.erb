<h1>Listing backend_designs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @backend_designs.each do |backend_design| %>
      <tr>
        <td><%= backend_design.name %></td>
        <td><%= link_to 'Show', backend_design %></td>
        <td><%= link_to 'Edit', edit_backend_design_path(backend_design) %></td>
        <td><%= link_to 'Destroy', backend_design, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Design', new_backend_design_path %>
